{

    "description": "Remove News on UCL dashboard and add loading spinner",
    "manifest_version": 3,
    "name": "UCL-noNews",
    "version": "1.0",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["https://www.uclouvain.be/fr/myuclouvain*"],
        "js": ["noNews.js"],
        "run_at": "document_start"
      }
    ]
  
  }